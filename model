   "source": [
    "def convert_to_float(x):\n",
    "    if is_float(x):\n",
    "        return x\n",
    "    tokens = x.split(\" - \")\n",
    "    if len(tokens) == 2:\n",
    "        return (float(tokens[0]) + float(tokens[1]))/2\n",
    "    try:\n",
    "        return float(x)\n",
    "    except:\n",
    "        return None\n",
    "        "
   ]
  },
  {
